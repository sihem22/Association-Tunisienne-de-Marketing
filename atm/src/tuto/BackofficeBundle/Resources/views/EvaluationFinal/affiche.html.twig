{% extends ":back:base.html.twig" %}
{% block title %}Evaluation Final{% endblock  %}
{% block body %}
    <div class="container">
        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <ol class="breadcrumb">
                        <li>
                            <i class="clip-home-2"></i>
                            <a href="{{path('tuto_frontoffice_homepage')}}">
                                Front Office
                            </a>
                        </li>
                        <li class="active">
                            <i><b>  Back Office </b> </i>
                        </li>

                    </ol>
                </div>
            </div>
        </div>
        {% include ":back:alert.html.twig" %}




        <div class="row">
            <div class="col-md-12">
                <!-- start: DYNAMIC TABLE PANEL -->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-external-link-square"></i>
                        Liste des Evaluations final
                        <div class="panel-tools">
                            <a class="btn btn-xs btn-link panel-collapse collapses" href="#"> </a>
                            <a class="btn btn-xs btn-link panel-expand" href="#"> <i class="fa fa-resize-full"></i> </a>

                        </div>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped table-bordered table-hover table-full-width" id="sample_1">
                            <thead>
                                <tr>
                                    <th class="center">Id</th>
                                    <th class="center">Soumission</th>
                                    
                                    <th class="center">A qui ?</th>
                                    <th class="center"> Note</th>
                                    <th class="center">  Résultat</th>
                                    <th class="center">  Evaluation1</th>
                                    <th class="center">  Evaluation2</th>
                                    <th class="center">  Evaluation3</th>
                                    <th class="center">  Action</th>



                                </tr>
                            </thead>
                            <tbody>
                                {% for evaluationfinal in evaluationfinal %}
                                    <tr>
                                        <td class="center">EvalFin_{{evaluationfinal.id}}</td> 
                                        <td class="center">{{evaluationfinal.soumission.id}}</td>

                                        <td class="center">{{evaluationfinal.soumission.user}}</td>
                                        <td class="center">{{evaluationfinal.note}}</td>
                                        <td class="center">{{evaluationfinal.resultat}}</td>


                                        <td class="center"> <object data="{{ asset(evaluationfinal.assetPath)}}"  type="application/pdf" width="25%" height="25%" >

                                            </object>
                                            <p><a href="{{ asset(evaluationfinal.assetPath)}}">click here to download the PDF file.</a> </p>

                                        </td>

                                        <td class="center"><object data="{{ asset(evaluationfinal.assetPath1)}}"  type="application/pdf" width="25%" height="25%"  >

                                            </object>
                                            <p><a href="{{ asset(evaluationfinal.assetPath1)}}">Cliquer ici pour téléchrger le fichier pdf</a> </p></td>

                                        <td class="center"> {% if evaluationfinal.path3 is not null %}<object data="{{ asset(evaluationfinal.assetPath2)}}"  type="application/pdf" width="25%" height="25%"  >

                                            </object>
                                            <p><a href="{{ asset(evaluationfinal.assetPath2)}}">Cliquer ici pour téléchrger le fichier pdf</a> </p>{%endif%}</td>



                                        <td class="center">


                                            <a href="{{path('tuto_evaluationfinal_supp',{'id':evaluationfinal.id})}}"  class="btn btn-xs btn-bricky tooltips"  data-original-title="Supprimer" data-placement="top" onclick="javascript:return(confirm('Voulez-vous  supprimer cette ligne??'))"><i class="fa fa-times fa fa-white"></i></a>

                                        </td>

                                    </tr>
                                    {%endfor%}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- end: DYNAMIC TABLE PANEL -->
                    </div>
                </div>
            </div>
            {% endblock  %}

